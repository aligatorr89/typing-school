{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [true, "single"],
    "no-console": [false, "log", "error"],
    "object-literal-sort-keys": false,
    "trailing-comma": [false],
    "prefer-for-of": false,
    "variable-name": {
      "options": [
        "allow-snake-case"
      ]
    },
    "max-classes-per-file": [true, 8, "exclude-class-expressions"]
  }
}
